<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	
	
    
    
    
    <class name="com.kerpach.components.entity.Department" table="Departments" catalog="Kerpach_ASC">
	
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        
        <property name="titleDepartmnet" type="string">
            <column name="title" length="45" not-null="true"/>
        </property>
        <property name="dateCreate" type="date">
            <column name="date_create" not-null="true"/>
        </property>
    </class>
    

</hibernate-mapping>